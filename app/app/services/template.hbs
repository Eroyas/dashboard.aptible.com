<div class="resource-list">
  {{#each model as |service|}}
    <div class="panel panel-default panel-subdued service">
      <div class="panel-heading">
        <h3>
          <span class="handle">{{service.handle}}</span>
          <span class="command">{{service.command}}</span>
        </h3>
      </div>
      <div class="panel-body">
        {{service-scaler service=service scaleService="scaleService"}}
      </div>
    </div>
  {{/each}}
</div>
