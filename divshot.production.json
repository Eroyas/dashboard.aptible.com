{
  "name": "diesel",
  "root": "./dist",
  "clean_urls": true,
  "routes": {
    "/tests": "tests/index.html",
    "/tests/**": "tests/index.html",
    "/**": "index.html"
  },
  "cache_control": {
    "/assets/**": 2592000,
    "/": false
  },
  "headers": {
    "*": {
      "Strict-Transport-Security": "max-age=31536000"
    }
  }
}
