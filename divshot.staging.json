{
  "name": "diesel-staging",
  "root": "./dist",
  "clean_urls": true,
  "routes": {
    "/tests": "tests/index.html",
    "/tests/**": "tests/index.html",
    "/**": "index.html"
  },
  "redirects": {
    "/apps": "/",
    "/apps/**": "/",
    "/databases": "/",
    "/databases/**": "/"
  },
  "cache_control": {
    "/assets/**": false,
    "/": false
  }
}
